{
  "name": "@usedapp/example",
  "version": "0.5.3",
  "private": true,
  "main": "dist/cjs/src/index.js",
  "module": "dist/esm/src/index.js",
  "types": "dist/esm/src/index.d.ts",
  "scripts": {
    "typechain:generate": "yarn typechain --target=ethers-v5 ./src/abi/**/*.json --out-dir=./gen/types",
    "usedapp:generate": "USEDAPP_OUT_DIR=./gen/hooks USEDAPP_TYPES_DIR=./gen/types pnpm usedapp-generate-hooks",
    "start": "webpack serve --mode development",
    "build": "yarn build:app && yarn build:package",
    "build:app": "yarn generate && tsc --noEmit && rimraf build && webpack --mode production --progress",
    "generate": "yarn typechain:generate && yarn usedapp:generate",
    "build:package": "yarn build:esm && yarn build:cjs && yarn copy-files",
    "build:esm": "tsc --module es2020 --target es2017 --outDir dist/esm",
    "build:cjs": "tsc --outDir dist/cjs",
    "lint": "yarn lint:prettier --check && yarn lint:eslint",
    "lint:fix": "yarn lint:prettier --write && yarn lint:eslint --fix",
    "lint:eslint": "eslint './{src,test,playwright}/**/*.{ts,tsx}'",
    "lint:prettier": "yarn prettier './{src,test,playwright}/**/*.{ts,tsx}'",
    "test": "mocha -r ts-node/register/transpile-only './test/**/*.test.{ts,tsx}' --timeout 60000",
    "test:playwright": "mocha -r ts-node/register/transpile-only './playwright/**/*.test.ts' --timeout 60000",
    "copy-files": "cp -r ./src/assets ./dist/cjs/src/assets && cp -r ./src/assets ./dist/esm/src/assets"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@ethersproject/abi": "5.6.1",
    "@ethersproject/contracts": "5.6.0",
    "@ethersproject/units": "5.6.0",
    "@types/styled-components": "^5.1.7",
    "@usedapp/coingecko": "workspace:*",
    "@usedapp/core": "workspace:*",
    "@walletconnect/web3-provider": "^1.7.1",
    "@web3-react/walletconnect-connector": "6.2.4",
    "debug": "^4.3.4",
    "file-loader": "^6.2.0",
    "framer-motion": "^4.1.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "^18.0.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "@ethersproject/providers": "5.6.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@swc-node/register": "^1.4.2",
    "@testing-library/react": "^11.0.0",
    "@typechain/ethers-v5": "10.0.0",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^8.2.0",
    "@types/node": "^17.0.10",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.1",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "4.2.6",
    "@usedapp/playwright": "workspace:*",
    "@usedapp/testing": "workspace:*",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^6.2.1",
    "esbuild-loader": "^2.11.0",
    "eslint": "7.22.0",
    "ethereum-waffle": "4.0.0-alpha.27",
    "html-webpack-plugin": "^4.5.0",
    "jsdom": "^20.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.2.1",
    "playwright": "^1.20.2",
    "prettier": "^2.0.5",
    "react-refresh": "^0.10.0",
    "rimraf": "^3.0.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "ts-loader": "^8.0.7",
    "ts-node": "^10.7.0",
    "typechain": "8.1.0",
    "typescript": "^4.6.2",
    "wait-for-expect": "^3.0.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  }
}
